{
  "name": "angular-ngrx-material-starter",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Angular, NgRx, Material design",
  "author": "Yurii Derevenko",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:prod": "npm run build:prod && npm run server",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "lint": "ng lint",
    "ci": "npm run format:test && ng lint && npm run build:prod -- --deploy-url /angular-ngrx-material-starter/ --base-href /angular-ngrx-material-starter",
    "format:write": "prettier projects/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier projects/**/*.{ts,json,md,scss} --list-different",
    "release": "standard-version && git push --follow-tags origin master",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/angular-ngrx-material-starter/stats-es2015.json",
    "server": "node ./projects/server/server.js"
  },
  "dependencies": {
    "@angular/animations": "~10.0.11",
    "@angular/cdk": "~10.1.3",
    "@angular/common": "~10.0.11",
    "@angular/compiler": "~10.0.11",
    "@angular/core": "~10.0.11",
    "@angular/forms": "~10.0.11",
    "@angular/material": "~10.1.3",
    "@angular/platform-browser": "~10.0.11",
    "@angular/platform-browser-dynamic": "~10.0.11",
    "@angular/router": "~10.0.11",
    "@angular/flex-layout": "~10.0.0-beta.32",
    "@ngrx/effects": "~10.0.0",
    "@ngrx/entity": "~10.0.0",
    "@ngrx/router-store": "~10.0.0",
    "@ngrx/store": "~10.0.0",
    "@ngrx/store-devtools": "~10.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "ngx-color-picker": "~10.0.1",
    "ngx-logger": "~4.1.9",
    "ngx-perfect-scrollbar": "~10.0.1",
    "lodash": "~4.17.20",
    "prismjs": "~1.21.0",
    "resize-observer-polyfill": "~1.5.1",
    "rxjs": "~6.6.2",
    "tslib": "~2.0.0",
    "web-animations-js": "~2.3.2",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.7",
    "@angular/cli": "~10.0.7",
    "@angular/compiler-cli": "~10.0.11",
    "@angular/language-service": "~10.0.11",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/lodash": "~4.14.159",
    "@types/node": "~14.6.0",
    "express": "^4.16.4",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "^7.0.0",
    "codelyzer": "~6.0.0",
    "rimraf": "~3.0.2",
    "standard-version": "^8.0.2",
    "tslint": "~6.1.2",
    "typescript": "~3.9.7",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}

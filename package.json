{
  "name": "angular-template",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Angular starter template (NgRx and Angular Material)",
  "author": "Yurii Derevenko",
  "private": true,
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist/",
    "start": "ng serve --open",
    "start:prod": "ng serve --prod --open",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "format:write": "prettier src/**/*.{ts,json,scss,html} --write",
    "analyze": "npm run clean && npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/stats-es2015.json",
    "lint": "eslint -c .eslintrc.js --ext .ts"
  },
  "dependencies": {
    "@angular/animations": "~10.0.11",
    "@angular/cdk": "~10.1.3",
    "@angular/common": "~10.0.11",
    "@angular/compiler": "~10.0.11",
    "@angular/core": "~10.0.11",
    "@angular/flex-layout": "~10.0.0-beta.32",
    "@angular/forms": "~10.0.11",
    "@angular/material": "~10.1.3",
    "@angular/platform-browser": "~10.0.11",
    "@angular/platform-browser-dynamic": "~10.0.11",
    "@angular/router": "~10.0.11",
    "@ngrx/effects": "~10.0.0",
    "@ngrx/entity": "~10.0.0",
    "@ngrx/router-store": "~10.0.0",
    "@ngrx/store": "~10.0.0",
    "@ngrx/store-devtools": "~10.0.0",
    "@ngx-translate/core": "~13.0.0",
    "@ngx-translate/http-loader": "~6.0.0",
    "lodash": "~4.17.20",
    "ngx-color-picker": "~10.0.1",
    "ngx-logger": "~4.1.9",
    "ngx-perfect-scrollbar": "~10.0.1",
    "resize-observer-polyfill": "~1.5.1",
    "rxjs": "~6.6.2",
    "tslib": "~2.0.0",
    "web-animations-js": "~2.3.2",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.7",
    "@angular/cli": "~10.0.7",
    "@angular/compiler-cli": "~10.0.11",
    "@angular/language-service": "~10.0.11",
    "@types/lodash": "~4.14.159",
    "@types/node": "~14.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "~3.9.1",
    "@typescript-eslint/eslint-plugin": "~3.9.1",
    "@typescript-eslint/parser": "~3.9.1",
    "eslint": "7.7.0",
    "tslint-to-eslint-config": "~1.3.0",
    "codelyzer": "~6.0.0",
    "prettier": "~2.0.5",
    "rimraf": "~3.0.2",
    "ts-node": "~9.0.0",
    "tslib": "~2.0.1",
    "typescript": "~3.9.7",
    "webpack-bundle-analyzer": "~3.8.0"
  }
}
